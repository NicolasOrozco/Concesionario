<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: #262626;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.viewController.HomeEmpleadoViewController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="600.0" prefWidth="500.0" style="-fx-background-color: #262626;">
         <children>
            <ImageView fx:id="imgConcesionario" fitHeight="610.0" fitWidth="526.0" layoutX="-10.0" layoutY="-10.0" opacity="0.52" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@carage-koenogsegg-dealer_2.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Button fx:id="btnRealizarTransaccion" layoutX="640.0" layoutY="300.0" mnemonicParsing="false" onAction="#onRealizarTransaccion" prefHeight="43.0" prefWidth="256.0" text="Realizar Transacción">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Tw Cen MT Condensed" size="24.0" />
         </font>
      </Button>
      <Button fx:id="btnComprarVehiculos" layoutX="640.0" layoutY="358.0" mnemonicParsing="false" onAction="#onComprarVehiculos" prefHeight="43.0" prefWidth="256.0" text="Comprar Vehiculos">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Tw Cen MT Condensed" size="24.0" />
         </font>
      </Button>
      <Label fx:id="lblPregunta" layoutX="695.0" layoutY="257.0" text="¿Qué desea hacer?" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed" size="26.0" />
         </font>
      </Label>
      <Label fx:id="lblBienvenida" layoutX="647.0" layoutY="129.0" text="Sesión iniciada como empleado" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed" size="26.0" />
         </font>
      </Label>
      <Label fx:id="lblTitulo" layoutX="657.0" layoutY="81.0" text="BIENVENIDO!" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="43.0" />
         </font>
      </Label>
      <Button fx:id="btnAgregarClientes" layoutX="640.0" layoutY="417.0" mnemonicParsing="false" onAction="#onAgregarClientes" prefHeight="43.0" prefWidth="256.0" text="Agregar Clientes">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Tw Cen MT Condensed" size="24.0" />
         </font>
      </Button>
      <Button fx:id="btnCerrarSesion" layoutX="640.0" layoutY="475.0" mnemonicParsing="false" onAction="#onCerrarSesion" prefHeight="43.0" prefWidth="256.0" text="Cerrar Sesión">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Tw Cen MT Condensed" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
